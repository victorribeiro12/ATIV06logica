programa {
  funcao inicio() {
  inteiro numero
  escreva("Digite um número: ")
    leia(numero)
     se ((numero < 50) ou (numero > 100))
        escreva("O número ", numero, " está fora do intervalo de 50 a 100.")
         senao
        escreva("O número ", numero, " está dentro do intervalo de 50 a 100.")
  }
}
